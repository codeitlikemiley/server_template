{
  "engine": "shopify",
  "files": {
    "settings": "",
    "locales": "",
    "sections": [],
    "snippets": []
  },
  "format": {
    "ignore": [],
    "wrap": 80,
    "indentSize": 4,
    "preserveLine": 2,
    "endNewline": true,
    "liquid": {
      "indentAttribute": true,
      "lineBreakSeparator": "before",
      "normalizeSpacing": true,
      "quoteConvert": "single",
      "ignoreTagList": []
    },
    "rust": {
      "bracePadding": false,
      "braceAllman": true,
      "arrayFormat": "indent",
      "objectIndent": "indent",
      "objectSort": false
    }
  },
  "markup": {
    "quoteConvert": "double",
    "selfCloseSpace": true,
    "delimiterTerminus": "inline",
    "stripAttributeLines": true,
    "commentNewline": false,
    "forceIndent": true,
    "commentIndent": true,
    "ignoreJS": true,
    "ignoreCSS": false,
    "ignoreJSON": false,
    "forceAttribute": 3
  },
  "json": {
    "bracePadding": false,
    "braceAllman": true,
    "arrayFormat": "indent",
    "objectIndent": "indent",
    "objectSort": false
  },
  "style": {
    "noLeadZero": true,
    "quoteConvert": "single",
    "atRuleSpace": true
  }
}
